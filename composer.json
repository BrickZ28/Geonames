{
  "name": "michaeldrennen/geonames",
  "description": "A Laravel (php) package that interfaces with the geolocation services on geonames.org.",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Michael Drennen",
      "email": "michaeldrennen74@gmail.com"
    }
  ],
  "require": {
    "php": "^8.0 || ^8.1 || ^8.2 || ^8.4", // Updated PHP version constraint
    "ext-intl": "*",
    "ext-pdo": "*",
    "ext-curl": "*",
    "laravel/framework": "^11.0", // Updated to support Laravel 11
    "michaeldrennen/remote-file": "^2.0",
    "michaeldrennen/local-file": "^2.0",
    "curl/curl": "^2.2",
    "fabpot/goutte": "^4.0",
    "nesbot/carbon": "^2.28",
    "illuminate/support": "^10.0 || ^11.0", // Updated for compatibility
    "illuminate/routing": "^10.0 || ^11.0",
    "illuminate/database": "^10.0 || ^11.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.0", // Updated PHPUnit version
    "orchestra/testbench": "^8.0", // Updated for compatibility with Laravel 11
    "php-coveralls/php-coveralls": "^2.2"
  },
  "autoload": {
    "psr-4": {
      "MichaelDrennen\\Geonames\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "MichaelDrennen\\Geonames\\Tests\\": "tests/"
    }
  },
  "minimum-stability": "stable",
  "extra": {
    "laravel": {
      "providers": [
        "MichaelDrennen\\Geonames\\GeonamesServiceProvider"
      ]
    }
  }
}
